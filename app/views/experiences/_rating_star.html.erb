<div class="stars-show-card" style= "font-size: 20px;">
  <% if @experience.average_rating %>
    <%  @experience.average_rating.times do %>
      <i class="fa-solid fa-star"></i>
    <% end %>
    <%  (5 - @experience.average_rating).times do %>
      <i class="fa-regular fa-star"></i>
    <% end %>
  <% else %>
    <%= "Unrated" %>
  <% end %>
</div>
